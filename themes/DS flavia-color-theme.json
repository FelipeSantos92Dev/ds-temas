    {
      "name": "Comments",
      "scope": [
        "comment",
        "punctuation.definition.comment",
        "string.quoted.docstring.multi",
        "comment.block.documentation source"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#b89ba6"
      }
    },
    {
      "name": "Constants",
      "scope": ["constant", "support.constant.core.php"],
      "settings": {
        "foreground": "#e65f7a"
      }
    },
    {
      "name": "Numbers and Booleans",
      "scope": [
        "constant.numeric",
        "constant.language",
        "support.constant",
        "constant.character",
        "constant.escape",
        "variable.other.constant",
        "punctuation.definition.constant"
      ],
      "settings": {
        "foreground": "#e65f7a"
      }
    },
    {
      "name": "Strings",
